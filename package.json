{
	"name": "@lesjoursfr/html-to-epub",
	"version": "2.1.0",
	"description": "A library to generate EPUB from HTML. Inspired by epub-gen.",
	"license": "MIT",
	"repository": "lesjoursfr/html-to-epub",
	"homepage": "https://github.com/lesjoursfr/html-to-epub#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/html-to-epub/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.15.0"
	},
	"keywords": [
		"epub",
		"generator",
		"ebook",
		"html"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*",
		"templates/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc",
		"test": "ava"
	},
	"dependencies": {
		"archiver": "^5.3.1",
		"axios": "^0.27.2",
		"cheerio": "^1.0.0-rc.11",
		"diacritics": "^1.3.0",
		"ejs": "^3.1.8",
		"entities": "^4.3.0",
		"fs-extra": "^10.1.0",
		"mime": "^3.0.0",
		"uslug": "^1.0.4"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/archiver": "^5.3.1",
		"@types/diacritics": "^1.3.1",
		"@types/ejs": "^3.1.1",
		"@types/fs-extra": "^9.0.13",
		"@types/mime": "^2.0.3",
		"@types/node": "^16.11.38",
		"@types/uslug": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"ava": "^4.2.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.6.2",
		"ts-node": "^10.8.0",
		"typescript": "^4.7.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.2.1"
}
