{
	"name": "@lesjoursfr/html-to-epub",
	"version": "2.0.5",
	"description": "A library to generate EPUB from HTML. Inspired by epub-gen.",
	"license": "MIT",
	"repository": "lesjoursfr/html-to-epub",
	"homepage": "https://github.com/lesjoursfr/html-to-epub#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/html-to-epub/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.14.1"
	},
	"keywords": [
		"epub",
		"generator",
		"ebook",
		"html"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*",
		"templates/**/*"
	],
	"scripts": {
		"build": "npx tsc",
		"lint": "npx eslint --ext .ts --fix src/",
		"test": "ava"
	},
	"dependencies": {
		"archiver": "^5.3.0",
		"axios": "^0.26.0",
		"cheerio": "^1.0.0-rc.10",
		"diacritics": "^1.3.0",
		"ejs": "^3.1.6",
		"entities": "^3.0.1",
		"fs-extra": "^10.0.1",
		"mime": "^3.0.0",
		"uslug": "^1.0.4"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/archiver": "^5.3.1",
		"@types/diacritics": "^1.3.1",
		"@types/ejs": "^3.1.0",
		"@types/fs-extra": "^9.0.13",
		"@types/mime": "^2.0.3",
		"@types/node": "^16.11.26",
		"@types/uslug": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"ava": "^4.0.1",
		"eslint": "^8.10.0",
		"ts-node": "^10.5.0",
		"typescript": "^4.6.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.2.0"
}
